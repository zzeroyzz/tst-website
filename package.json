{
  "name": "tst",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:verbose": "NODE_ENV=test TEST_VERBOSE=true jest --verbose",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand --no-cache",
    "test:update-snapshots": "jest --updateSnapshot",
    "test:clear-cache": "jest --clearCache",
    "cy:open": "start-server-and-test 'npm run dev' http://localhost:3000 'cypress open'",
    "cy:run": "start-server-and-test 'npm run dev' http://localhost:3000 'cypress run'",
    "cypress:run:component": "cypress run --component",
    "test:e2e": "cypress run --spec 'cypress/e2e/**/*'",
    "test:component": "cypress run --component --spec 'cypress/component/**/*'",
    "e2e:ci": "start-server-and-test 'npm run start:test' http://localhost:3000 'cypress run --browser chrome --headless'",
    "e2e:ci:component": "cypress run --component --browser chrome --headless",
    "codegen": "graphql-codegen --config codegen.ts",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "@apollo/client": "^4.0.0",
