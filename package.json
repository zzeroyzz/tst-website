    "framer-motion": "^12.23.3",
    "graphql": "^16.11.0",
    "graphql-tag": "^2.12.6",
    "gsap": "^3.13.0",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.525.0",
    "marked": "^16.0.0",
